#!/usr/bin/env bash
# displaying numbers

a=1

while [ $a -lt 21 ]
do
	case ${a} in
	     4)
	     echo "$a"
	     echo "bad luck from China"
	     (( a++  ))
	     continue
	     ;;
	     9)
	     echo "$a"
	     echo "bad luck from Japan"
	     (( a++  ))
	     continue
	     ;;
	     17)
	     echo "$a"
	     echo "bad luck from Italy"
	     (( a++  ))
	     continue
	     ;;
	     *)
	     echo "$a"
	     (( a++ ))
	     ;;
done
